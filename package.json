{
  "name": "@devnetic/utils",
  "version": "1.2.0",
  "description": "Common utils for every single day tasks",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run build -- -w",
    "clean": "rm -rf lib",
    "coverage": "nyc ava",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "prepublishOnly": "npm run build && npm run lint && npm run coverage",
    "release": "standard-version",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "ts-standard": {
    "ignore": [
      "lib",
      "examples",
      "test"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devnetic/utils.git"
  },
  "keywords": [
    "utils",
    "camelCase",
    "camel case",
    "kebab-case",
    "kebab case",
    "snake_case",
    "Snake Case",
    "match all",
    "matchAll",
    "dateFormat",
    "isEqual",
    "msToTime",
    "time format",
    "uuid",
    "setValue"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": "Álvaro José Agámez Licha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devnetic/utils/issues"
  },
  "homepage": "https://github.com/devnetic/utils#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/format": "^11.0.0",
    "@types/node": "^14.14.6",
    "ava": "^3.13.0",
    "husky": "^4.3.0",
    "nyc": "^15.1.0",
    "standard-version": "^9.0.0",
    "ts-standard": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {}
}
